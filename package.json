{
  "name": "bond-gatsby-starter",
  "private": true,
  "description": "Gatsby starter for Bond London",
  "version": "1.1.1",
  "license": "0BSD",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prebuild": "yarn lint",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "yarn  develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --max-warnings 0 \"**/*.{js,jsx,ts,tsx}\"",
    "clean:generated": "rimraf graphcms-fragments src/generated",
    "css": "tailwind build src/styles/index.css -o built.css",
    "restart": "run-s clean:generated develop",
    "restart:full": "run-s clean clean:generated develop",
    "build:full": "run-s clean clean:generated build",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@bond-london/gatsby-graphcms-components": "^1.5.3",
    "@bond-london/gatsby-plugin-cookie-scripts": "^2.0.0",
    "@bond-london/gatsby-plugin-generate-typings": "^1.1.0",
    "@bond-london/gatsby-transformer-expand": "^1.0.2",
    "@bond-london/gatsby-transformer-extracted-lottie": "^1.2.0",
    "@bond-london/gatsby-transformer-extracted-svg": "^1.2.0",
    "@fontsource/poppins": "^4.5.0",
    "@graphcms/rich-text-react-renderer": "^0.3.2",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "@types/react": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "autoprefixer": "^10.3.7",
    "babel-eslint": "^10.1.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby": "^4.0.1",
    "gatsby-plugin-eslint": "^4.0.0",
    "gatsby-plugin-gatsby-cloud": "^4.0.0",
    "gatsby-plugin-image": "^2.0.0",
    "gatsby-plugin-manifest": "^4.0.0",
    "gatsby-plugin-page-creator": "^4.0.0",
    "gatsby-plugin-postcss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^5.0.0",
    "gatsby-plugin-sharp": "^4.0.1",
    "gatsby-plugin-sitemap": "^5.0.0",
    "gatsby-source-filesystem": "^4.0.0",
    "gatsby-transformer-json": "^4.0.0",
    "gatsby-transformer-sharp": "^4.0.0",
    "lmdb-store": "^1.6.11",
    "lottie-web": "^5.7.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "pure-react-carousel": "^1.27.6",
    "react": "^17.0.1",
    "react-cookie-consent": "^6.4.1",
    "react-dom": "^17.0.1",
    "react-responsive": "^9.0.0-beta.4",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.17",
    "tailwindcss-debug-screens": "^2.2.0",
    "typescript": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bond-london/bond-gatsby-starter"
  },
  "bugs": {
    "url": "https://github.com/bond-london/bond-gatsby-starter/issues"
  },
  "devDependencies": {
    "hint": "^6.1.6"
  }
}
