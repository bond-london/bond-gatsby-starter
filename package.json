{
  "name": "bond-gatsby-starter",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "description": "Gatsby starter for Bond London",
  "version": "1.1.1",
  "license": "0BSD",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prebuild": "yarn lint",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "yarn  develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --max-warnings 0 \"**/*.{js,jsx,ts,tsx}\"",
    "clean:generated": "rimraf graphcms-fragments src/generated",
    "css": "tailwind build src/styles/index.css -o built.css",
    "restart": "run-s clean:generated develop",
    "restart:full": "run-s clean clean:generated develop",
    "build:full": "run-s clean clean:generated build",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@bond-london/gatsby-graphcms-components": "^1.8.8",
    "@bond-london/gatsby-plugin-cookie-scripts": "^2.0.0",
    "@bond-london/gatsby-plugin-generate-typings": "^2.0.0",
    "@bond-london/gatsby-transformer-expand": "^2.0.0",
    "@bond-london/gatsby-transformer-extracted-lottie": "^2.0.1",
    "@bond-london/gatsby-transformer-extracted-svg": "^2.0.0",
    "@bond-london/graphcms-rich-text": "^0.2.3",
    "@fontsource/poppins": "^4.5.0",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/component": "^5.15.0",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/loadable__component": "^5.13.4",
    "@types/react": "^17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "autoprefixer": "^10.3.7",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gatsby": "^4.2.0",
    "gatsby-plugin-eslint": "^4.0.2",
    "gatsby-plugin-gatsby-cloud": "^4.2.0",
    "gatsby-plugin-image": "^2.2.0",
    "gatsby-plugin-loadable-components-ssr": "^4.1.0",
    "gatsby-plugin-manifest": "^4.2.0",
    "gatsby-plugin-page-creator": "^4.2.0",
    "gatsby-plugin-postcss": "^5.2.0",
    "gatsby-plugin-react-helmet": "^5.2.0",
    "gatsby-plugin-sharp": "^4.2.0",
    "gatsby-plugin-sitemap": "^5.2.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.26",
    "gatsby-source-filesystem": "^4.2.0",
    "gatsby-transformer-json": "^4.2.0",
    "gatsby-transformer-sharp": "^4.2.0",
    "lmdb-store": "^1.6.14",
    "lottie-web": "^5.8.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.1",
    "prettier": "^2.5.0",
    "pure-react-carousel": "^1.27.8",
    "react": "^17.0.1",
    "react-cookie-consent": "^6.4.1",
    "react-dom": "^17.0.1",
    "react-responsive": "^9.0.0-beta.5",
    "react-select": "^5.2.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.17",
    "tailwindcss-debug-screens": "^2.2.0",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bond-london/bond-gatsby-starter"
  },
  "bugs": {
    "url": "https://github.com/bond-london/bond-gatsby-starter/issues"
  },
  "devDependencies": {
    "hint": "^6.1.9"
  }
}
