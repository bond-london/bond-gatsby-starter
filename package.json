{
  "name": "bond-gatsby-starter",
  "private": true,
  "description": "Gatsby starter for Bond London",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "clean:generated": "rimraf graphcms-fragments src/generated",
    "css": "tailwind build src/styles/index.css -o built.css",
    "restart": "run-s clean:generated develop",
    "restart:full": "run-s clean clean:generated develop",
    "build:full": "run-s clean clean:generated build",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@bond-london/gatsby-plugin-generate-typings": "^0.9.13",
    "@bond-london/gatsby-transformer-extracted-lottie": "^1.0.3",
    "@bond-london/gatsby-transformer-extracted-svg": "^1.0.5",
    "@fontsource/roboto": "^4.4.5",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@types/react-helmet": "^6.1.1",
    "autoprefixer": "^10.2.6",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "gatsby": "^3.6.2",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-gatsby-cloud": "^2.8.1",
    "gatsby-plugin-image": "^1.8.0",
    "gatsby-plugin-manifest": "^3.8.0",
    "gatsby-plugin-postcss": "^4.8.0",
    "gatsby-plugin-react-helmet": "^4.8.0",
    "gatsby-plugin-sharp": "^3.8.0",
    "gatsby-plugin-sitemap": "^4.4.0",
    "gatsby-source-filesystem": "^3.8.0",
    "gatsby-transformer-json": "^3.8.0",
    "gatsby-transformer-sharp": "^3.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.2.4",
    "tailwindcss-debug-screens": "^2.0.0",
    "typescript": "^4.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bond-london/bond-gatsby-starter"
  },
  "bugs": {
    "url": "https://github.com/bond-london/bond-gatsby-starter/issues"
  }
}
